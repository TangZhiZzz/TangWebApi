{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "FilePath": "logs/app-.log"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=TangWebApi.db",
    "Redis": "localhost:6379,abortConnect=false"
  },
  "JwtSettings": {
    "SecretKey": "TangWebApi_JWT_Secret_Key_2024_Very_Long_And_Secure_Key_For_Production_Use",
    "Issuer": "TangWebApi",
    "Audience": "TangWebApi.Users",
    "ExpiryInMinutes": 60
  },
  "MessageQueue": {
    "ConnectionString": "amqp://guest:guest@localhost:5672/",
    "DefaultExchange": "TangWebApi.Exchange",
    "DefaultRoutingKey": "default",
    "RetryCount": 3,
    "RetryDelayMs": 1000,
    "ConnectionTimeoutMs": 30000,
    "RequestedHeartbeat": 60,
    "Queues": [
      {
        "Name": "test-queue",
        "Description": "测试队列",
        "Enabled": true
      },
      {
        "Name": "email-queue",
        "Description": "邮件发送队列",
        "Enabled": true
      },
      {
        "Name": "notification-queue",
        "Description": "通知队列",
        "Enabled": true
      },
      {
        "Name": "log-queue",
        "Description": "日志处理队列",
        "Enabled": false
      }
    ]
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "EnableSsl": true,
    "Username": "your-email@gmail.com",
    "Password": "your-app-password",
    "FromEmail": "your-email@gmail.com",
    "FromName": "TangWebApi System",
    "Timeout": 30000
  },
  "OpenAISettings": {
    "ApiKey": "sk-wbycbifibhmxpgauokagpacipbdpqkivveelknvsvqcihdqi",
    "BaseUrl": "https://api.siliconflow.cn/v1",
    "DefaultModel": "deepseek-ai/DeepSeek-V3.1",
    "MaxTokens": 1000,
    "Temperature": 0.7,
    "TimeoutSeconds": 30,
    "Enabled": true
  },
  "FileSettings": {
    "UploadPath": "uploads",
    "MaxFileSize": 10485760,
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".gif", ".pdf", ".doc", ".docx", ".xls", ".xlsx", ".txt", ".zip", ".rar"],
    "EnableCompression": true,
    "CompressionQuality": 85,
    "GenerateThumbnail": true,
    "ThumbnailSize": 200,
    "BaseUrl": "http://localhost:5238",
    "EnableMd5Check": true,
    "AutoCleanupDays": 30
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 1000
      }
    ]
  },
  "ClientRateLimiting": {
    "ClientIdHeader": "X-ClientId",
    "EnableEndpointRateLimiting": true,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 200
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 2000
      }
    ],
    "HttpStatusCode": 429,
    "StackBlockedRequests": false
  },
  "DistributedLockSettings": {
    "Enabled": true,
    "DefaultExpirationSeconds": 30,
    "RetryIntervalMs": 100,
    "MaxRetryCount": 50,
    "KeyPrefix": "lock:",
    "EnableAutoRenewal": true,
    "AutoRenewalIntervalMs": 10000,
    "RenewalThresholdMs": 5000,
    "EnableMonitoring": true,
    "SlowLockThresholdMs": 1000,
    "OwnerIdentifierStrategy": "guid",
    "CustomOwnerIdentifier": ""
  }
}
